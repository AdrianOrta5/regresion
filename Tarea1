##Ejercicio 1
vectorz<-11:30
dim(vectorz)<-c(5,4)
View(vectorz)
rownames(vectorz)<-c("escuela1","escuela2","escuela3","escuela4","escuela5")
colnames(vectorz)<-c("toluca","pachuca","gdl","mty")
View(vectorz)
vectorz[3,1]
vectorz[5,3]
vectorz[1,2]

